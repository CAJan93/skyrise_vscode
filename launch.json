{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Attach",
      "type": "gdb",
      "request": "attach",
      "executable": "${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}", // "${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}",
      "target": "192.168.97.2:2345",
      "remote": true,
      "autorun": [
        "add-symbol-file ${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}", //  ${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}",
        "directory ${workspaceRoot}/src/${input:pickFolder}", // ${workspaceRoot}/src/operators/scan_operator/",
        "b main"
      ],
      "cwd": "${workspaceRoot}",
      "gdbpath": "/usr/bin/gdb",
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "pickFolder",
      "description": "The directory of the source files inside /src/",
      "options": [
        "benchmark/",
        "operators/scan_operator/"
      ]
    },
    {
      "type": "pickString",
      "id": "pickArtifact",
      "description": "The artifact to debug inside folder cmake-build-debug/bin",
      "options": [
        "skyriseMicroBenchmarks",
        "scanOperator"
      ]
    }
  ]
}