{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Attach",
      "type": "gdb",
      "request": "attach",
      "executable": "${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}", // "${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}",
      "target": "127.0.0.1:1234",
      "remote": true,
      "autorun": [
        "set solib-search-path ${workspaceRoot}/cmake-build-debug/lib/",
        "add-symbol-file ${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}", //  ${workspaceRoot}/cmake-build-debug/bin/${input:pickArtifact}",
        "add-symbol-file ${workspaceRoot}/cmake-build-debug/lib/libskyrise.so",
        "directory ${workspaceRoot}/src/lib",
        "directory ${workspaceRoot}/src/lib/utils",
        "directory ${workspaceRoot}/src/lib/expression",
        "directory ${workspaceRoot}/src/lib/expression/evaluation",
        "directory ${workspaceRoot}/src/test",
        "directory ${workspaceRoot}/src/test/utils",
        "directory ${workspaceRoot}/src/test/expression",
        "directory ${workspaceRoot}/src/operators",
        "directory ${workspaceRoot}/src/operators/buffer",
        "directory ${workspaceRoot}/src/operators/communicator",
        "directory ${workspaceRoot}/src/operators/scan_operator",
        "directory ${workspaceRoot}/src/operators/orchestrator",
        "set substitute-path /tmp/ /home/Jan.Mensch/Documents/skyrise/",
        "b main"
      ],
      "cwd": "${workspaceRoot}",
      "gdbpath": "/usr/bin/gdb"
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "pickArtifact",
      "description": "The artifact to debug inside folder cmake-build-debug/bin",
      "options": [
        "skyriseMicroBenchmarks",
        "scanOperator",
        "benchmarkDriver",
        "jsonSerializerTest",
        "evaluatorTest"
      ]
    }
  ]
}
